{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["uploads/**"] }
      }
    ],
    "routes": [
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  